<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark">

    <fx:Metadata>
        [HostComponent("spark.components.Button")]
    </fx:Metadata> 
    
    <s:states>
        <s:State name="up" />
        <s:State name="down" />
        <s:State name="over" />
        <s:State name="disabled" />
    </s:states>

    <!-- border and fill -->
    <s:Rect left="0" right="0" top="0" bottom="0" radiusX="2" radiusY="2">
        <s:stroke>
            <s:SolidColorStroke color="0xA34A64" />
        </s:stroke>
        <s:fill>
            <s:LinearGradient>
                <s:GradientEntry color="0xD5F8B4" />
                <s:GradientEntry color="0xB8EB8B" />
            </s:LinearGradient>
        </s:fill>
    </s:Rect>
</s:Skin>